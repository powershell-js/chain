.tost{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  background: rgba(0,0,0,0.4);
  display: flex;
  align-items: center;
  justify-content: center;
}
.tost_list{
  width: 40%;
  text-align: center;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  border-radius: 10rpx;
}
page{
  background: rgb(241,240,245);
  font-size:24rpx;

}
.heads{
  width: 100%;
  height: 10vh;
  
}
.head{
  font-size: 28rpx;
  position: fixed;
  top: 0;
  width: 100%;
  height: 10vh;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items:center;/*垂直居中*/
  /*justify-content: center;水平居中*/
  background: #fff;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.head>view{
  display: flex;
  flex-direction: row;
  align-items:center;/*垂直居中*/
}
.head_img>image{
  width:30rpx;
  height: 12rpx;
  margin-left: 16rpx;
}
.subject{
  width: 100%;
  height: 90vh;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  padding: 20rpx;
}
.conter{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.left_list>view>view{
  margin-top: 14rpx;
}
.time>view{
  margin-top: 14rpx;
}
.time>view{
  margin-top: 14rpx;
}
.list{
  width: 100%;
  /* height: 100rpx; */
  box-sizing: border-box;
  /* border:1rpx solid red; */
  background: #fff;
  margin-bottom: 20rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items:center;/*垂直居中*/

  -webkit-box-shadow: rgb(214, 218, 228) 0px 0px 10px;
   -moz-box-shadow: rgb(214, 218, 228) 0px 0px 10px;
   box-shadow: rgb(214, 218, 228) 0px 0px 10px;
}
.left_img{
  width:100rpx;
  height:115rpx;
  margin-right: 22rpx;
}
.iphoto{
  width:31rpx;
  height:31rpx;
}
.pretend{
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  justify-content: space-between;
  color:rgb(136, 136, 136);
}
.contacts{
  color:#91BDFB;
}
.adress{
  display: flex;
  flex-direction: row;
  align-items:center;/*垂直居中*/
  color:rgb(136, 136, 136);
}
.time{
  color:rgb(136, 136, 136);
}
.adress_img{
  width:20rpx;
  height:20rpx;
  margin-right: 16rpx;
}
.logion{
  width: 100%;
  position: fixed;
  height: 100vh;
  box-sizing: border-box;
  border:1rpx solid red;
  top:-100vh;
  overflow: hidden;
  background: #fff;
}
.car{
  display: flex;
  flex-direction: row;
  color:rgb(136, 136, 136);
}
.car>view:nth-child(1){
  width: 74rpx;
}
.car_1{
  word-break: break-all;  
  white-space: pre-wrap;
}
.adress>view{
  word-break: break-all;  
  white-space: pre-wrap;
}
.pretend>view:nth-child(1){
  margin-right: 30rpx;
}
.city{
  font-size: 30rpx;
  color:rgb(51, 51, 51);
  font-weight: 600;
}
.white{
  color:#fff;
}
.companys{
  margin-left: 16rpx;
  width: 25rpx;
  height: 26rpx;
}
.left_list{
  display: flex;
  flex-direction: row;
  align-items:center;/*垂直居中*/
}